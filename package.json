{
  "name": "moleculer-store",
  "version": "1.4.0",
  "description": "Key-Value storage service for Moleculer microservices with a built-in memory and Redis adapter",
  "main": "build/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "prettify": "prettier --write .",
    "build": "tsc",
    "build:watch": "tsc -w",
    "precommit": "npm run lint && npm run prettify",
    "clean": "rm -rf build",
    "prepublish": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest \"src/.*\\.unit\\.test\\.ts\"",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:int": "jest \"src/.*\\.int\\.test\\.ts\"",
    "test:int:watch": "npm run test:int -- --watch"
  },
  "keywords": [
    "moleculer",
    "microservices",
    "key-value store",
    "memory store",
    "redis"
  ],
  "author": "Waddah-JD",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Waddah-JD/moleculer-store.git"
  },
  "peerDependencies": {
    "moleculer": "^0.12.0 || ^0.13.0 || ^0.14.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.40",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^8.17.0",
    "jest": "^27.3.1",
    "moleculer": "^0.14.17",
    "prettier": "^2.4.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "redis": "^4.1.0"
  }
}
